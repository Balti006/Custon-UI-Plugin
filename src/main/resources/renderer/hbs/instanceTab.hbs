 <div>
        <h2>OverView</h2>
        <div class="tab-header-stats">
            <div class="tab-header-stat-item big-stat-block">
                <span class="stat-content">{{name}}</span>
                <span class="stat-label">Name</span>

            </div>
            <div class="tab-header-stat-item big-stat-block">
                <span class="stat-content">{{status}}</span>
                <span class="stat-label">Status</span>

            </div>
            <div class="tab-header-stat-item big-stat-block">
                <span class="stat-content">{{servicesCount}}</span>
                <span class="stat-label">No. of Services</span>

            </div>
            <div class="tab-header-stat-item big-stat-block">
                <span class="stat-content">{{datacenter}}</span>
                <span class="stat-label">Datacenter</span>

            </div>

        </div>
        <h2>Node Metedata</h2>
        <table>
            <thead>
            <th>Key</th>
            <th>Value</th>
            </thead>
            <tbody>
                {{#each metadta}}
                    <tr>
                        <td>{{@key}}</td>
                        <td>{{this}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
        <h2>Services</h2>
        <table>
            <thead>
            <th>Name</th>
            <th>Services</th>
            <th>Port</th>
            </thead>
            <tbody>
                {{#each metadta}}
                    <tr>
                        <td>{{this.ID}}</td>
                         <td>{{this.Services}}</td>
                        <td>{{this.Port}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
<style>
    .big-stat-block{
        margin-top: 15px;
        margin-bottom: 40px;
    }
</style>
